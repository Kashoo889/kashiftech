{
  "buildCommand": "pnpm build:frontend",
  "outputDirectory": "dist/public",
  "devCommand": "pnpm dev:frontend",
  "installCommand": "pnpm install",
  "framework": "vite",
  "functions": {
    "api/chat.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

